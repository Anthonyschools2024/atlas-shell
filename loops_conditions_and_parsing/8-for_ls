#!/usr/bin/env bash
# Iterate over all items in the current directory
for item in *; do
    # Check if the item is a hidden file (starts with a dot)
    if [[ $item ==.* ]]; then
        continue
    fi

    # Extract the part of the name after the first dash
    # If there is no dash, just print the item name
    echo "${item##*-}"
done
